{
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "zig": "zig run",
    "cpp": "cls && cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runghc",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName"
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "terminal.integrated.allowMnemonics": true,
  "files.autoSave": "afterDelay",
  "code-runner.runInTerminal": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": " Iosevka, JetBrains Mono Regular ,MonoLisa ,Consolas ligaturized v3, Fira Code , SF Mono , 'Courier New' , monospace , Pragmata Pro , Cascadia Code",
  "editor.mouseWheelZoom": true,
  "editor.minimap.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.fontWeight": "normal",
  "code-runner.clearPreviousOutput": true,
  "editor.largeFileOptimizations": false,
  "editor.indentSize": "tabSize",
  "editor.codeActionsOnSave": {},
  "editor.formatOnSave": true,
  "editor.detectIndentation": false,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "terminal.integrated.cursorWidth": 3,
  "terminal.integrated.cursorStyle": "line",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}${AppName}",
  "workbench.layoutControl.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "editor.fontSize": 25,
  "editor.cursorWidth": 6,
  "editor.cursorBlinking": "solid",
  "window.menuBarVisibility": "hidden",
  "C_Cpp.formatting": "vcFormat",
  "editor.cursorSmoothCaretAnimation": "on",
  "window.commandCenter": false,
  "window.zoomLevel": -1,
  "breadcrumbs.enabled": false,
  "editor.mouseWheelScrollSensitivity": 1.25,
  "workbench.activityBar.location": "hidden",
  "editor.cursorSurroundingLines": 1,
  "editor.suggestFontSize": 25,
  "editor.suggestLineHeight": 25,
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.letterSpacing": 1,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.verticalScrollbarSize": 15,
  "explorer.compactFolders": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabActionCloseVisibility": false,
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.intelliSenseUpdateDelay": 1000,
  "C_Cpp.autocomplete": "disabled",
  "editor.inlayHints.enabled": "off",
  "editor.fontLigatures": false,
  "editor.tabSize": 2,
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "files.autoSaveDelay": 500,
  "editor.quickSuggestionsDelay": 100,
  "workbench.colorTheme": "Visual Studio Dark",
  "editor.lineHeight": 35, //36
  "workbench.sideBar.location": "right",
  "editor.scrollbar.horizontalScrollbarSize": 15,
  "C_Cpp.files.exclude": {
    "**/.vscode": true,
    "**/.vs": true
  },
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#4f4f4f95",
    "editor.inactiveSelectionBackground": "#3e3e3e95",
    "editorLineNumber.foreground": "#6b6b6b95",
    "editorGutter.background": "#000000",
    "editor.background": "#000000",
    "editorCursor.foreground": "#ffa600",
    "editorLineNumber.activeForeground": "#ffa600",
    "sideBar.background": "#000000",
    "sideBar.border": "#3e3e3e",
    "editorGroup.border": "#3e3e3e",
    "panel.border": "#3e3e3e",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#ffffff",
    "editorError.background": "#ff000053",
    "editorError.foreground": "#ff0000",
    // "tab.activeBorder": "#4285f4",
    // "tab.activeBorderTop": "#4285f4",
    "tab.inactiveBackground": "#000000",
    "tab.activeBackground": "#262626",
    "editorGroupHeader.tabsBackground": "#000000",
    // "scrollbarSlider.hoverBackground": "#222222a4",
    "scrollbarSlider.activeBackground": "#7a7e85a4",
    "statusBar.background": "#000000", //17191A
    "statusBarItem.remoteBackground": "#000000"
  },
  "editor.tokenColorCustomizations": {
    "comments": "#52576c",
    "strings": "#59CBB5",
    "functions": "#ddd49a",
    "variables": "#FFF",
    "types": "#61b8ff",
    "keywords": "#61b8ff",
    "numbers": "#c3e88d",
    "textMateRules": [
      { // int like keywords which are not coloring blue, has been explicitly turned blue
        "scope": [
          "entity.name.function.preprocessor.cpp",
          "keyword.other.typedef.cpp",
          "keyword.other.using.directive.cpp",
          "meta.preprocessor.macro.cpp",
        ],
        "settings": {
          "foreground": "#61b8ff"
        }
      },
      { //const, GGC optimize, static keyword
        "scope": [
          "storage.modifier.specifier.const.cpp",
          "entity.other.attribute-name.pragma.preprocessor.cpp",
          "storage.modifier.static.cpp",
        ],
        "settings": {
          "foreground": "#c792ea"
        }
      },
      {
        "name": "Comment",
        "scope": [
          "comment",
        ],
        "settings": {
          "fontStyle": "italic",
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#C688C1",
        }
      },
      {
        "scope": "punctuation.separator",
        "settings": {
          "foreground": "#FFF",
          "fontStyle": "bold"
        }
      },
    ]
  },
  "editor.suggest.showClasses": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showDeprecated": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showEnums": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showFolders": false,
  "editor.suggest.showFunctions": false,
  "editor.suggest.showInterfaces": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showMethods": false,
  "editor.suggest.showModules": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showProperties": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showTypeParameters": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showValues": false,
  "editor.suggest.showVariables": false,
  "editor.suggest.showWords": false,
}